<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.santabooks.mypage.dao.face.MypageDao">

<select id="selectById" parameterType="String" resultType="com.santabooks.member.dto.Member">
		SELECT memberName, memberId, memberNick FROM member
		WHERE 1=1
			AND memberId = #{value }
			AND memberId = #{memberId }
</select>
	
	
</mapper>